<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#FFFFFF"
    tools:context="eletrofisio.com.teste11.fragments.AparelhosFragment">


    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:text="APARELHOS"
        android:textAlignment="center"
        android:textColor="#013105"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botao_tens"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/botao_ultra_som"
        android:layout_marginBottom="23dp"
        android:layout_toLeftOf="@+id/botaoVoltar"
        android:layout_toStartOf="@+id/botaoVoltar"
        android:background="@drawable/fundo_caixa_texto"
        android:text="tens"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botao_fes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/botao_tens"
        android:layout_alignBottom="@+id/botao_tens"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_toEndOf="@+id/textView23"
        android:layout_toRightOf="@+id/textView23"
        android:background="@drawable/fundo_caixa_texto"
        android:text="fes"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botao_ultra_som"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/botaoOndasCurtas"
        android:layout_alignLeft="@+id/botao_tens"
        android:layout_alignStart="@+id/botao_tens"
        android:layout_marginBottom="36dp"
        android:background="@drawable/fundo_caixa_texto"
        android:text="ultra-som"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botaoLaser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/botao_fes"
        android:layout_alignStart="@+id/botao_fes"
        android:layout_alignTop="@+id/botao_ultra_som"
        android:background="@drawable/fundo_caixa_texto"
        android:text="laser"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botaoOndasCurtas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/botaoInterferencial"
        android:layout_alignLeft="@+id/botao_ultra_som"
        android:layout_alignStart="@+id/botao_ultra_som"
        android:layout_marginBottom="30dp"
        android:background="@drawable/fundo_caixa_texto"
        android:text="ondas curtas"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botaoRussa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/botaoLaser"
        android:layout_alignStart="@+id/botaoLaser"
        android:layout_alignTop="@+id/botaoOndasCurtas"
        android:background="@drawable/fundo_caixa_texto"
        android:text="russa"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botaoInterferencial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/botaoVoltar"
        android:layout_alignLeft="@+id/botaoOndasCurtas"
        android:layout_alignStart="@+id/botaoOndasCurtas"
        android:layout_marginBottom="49dp"
        android:background="@drawable/fundo_caixa_texto"
        android:text="interferencial"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botao_aussie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/botaoRussa"
        android:layout_alignStart="@+id/botaoRussa"
        android:layout_alignTop="@+id/botaoInterferencial"
        android:background="@drawable/fundo_caixa_texto"
        android:text="aussie"
        android:textColor="#013105"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botaoVoltar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp"
        android:background="@drawable/fundo_caixa_texto"
        android:text="voltar para inÃ­cio"
        android:textColor="#013105"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/botaoInterferencial"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="7dp"
        app:srcCompat="@drawable/teste5" />


</RelativeLayout>

