<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="eletrofisio.com.teste11.activity.Laser.DosimetriaLaserActivity">


    <TextView
        android:id="@+id/textView93"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp"
        android:text="Dosimetria do Laser"
        android:textColor="#013105"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView104"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView93"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="114dp"
        android:text="- Ação antinflamatória ..........1 a 3 J/cm2"
        android:textColor="#013105"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView105"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView104"
        android:layout_alignLeft="@+id/textView104"
        android:layout_alignRight="@+id/textView104"
        android:layout_alignStart="@+id/textView104"
        android:layout_below="@+id/textView104"
        android:layout_marginTop="40dp"
        android:text="- Ação circulatória ............... 1 a 3 J/cm2 "
        android:textColor="#013105"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView106"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView105"
        android:layout_alignLeft="@+id/textView105"
        android:layout_alignRight="@+id/textView105"
        android:layout_alignStart="@+id/textView105"
        android:layout_below="@+id/textView105"
        android:layout_marginTop="39dp"
        android:text="- Ação antálgica .................. 2 a 4 J/cm2 "
        android:textColor="#013105"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView107"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView106"
        android:layout_alignStart="@+id/textView106"
        android:layout_below="@+id/textView106"
        android:layout_marginTop="39dp"
        android:text="- Ação regenerativa ..............3 a 6 J/ cm2 "
        android:textColor="#013105"
        android:textSize="18sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botao_voltar_laser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="61dp"
        android:background="@drawable/fundo_caixa_texto"
        android:text="voltar"
        android:textColor="#013105" />
</RelativeLayout>
