<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tens"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"

    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".activity.TENS.TensActivity">

    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="7dp"
        android:layout_marginStart="7dp"
        android:background="#FFFFFF"
        android:orientation="vertical">

    </LinearLayout>

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="TENS"
        android:textColor="#013105"
        android:textSize="30sp"
        android:textStyle="normal|bold" />

    <Button
        android:id="@+id/botaoConvencional"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/botaoBreveIntensa"
        android:layout_alignBottom="@+id/botaoBreveIntensa"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="Convencional" />

    <Button
        android:id="@+id/botaoBreveIntensa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView22"
        android:layout_marginTop="54dp"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="Breve/Intensa" />

    <Button
        android:id="@+id/botaoBurst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/botaoConvencional"
        android:layout_marginTop="79dp"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="BURST" />

    <Button
        android:id="@+id/botaoAcupuntural"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/botaoBurst"
        android:layout_alignBottom="@+id/botaoBurst"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="Acupuntural" />


    <Button
        android:id="@+id/botaoIndicacoes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/id_botaoVoltar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="67dp"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="Indicações" />

    <Button
        android:id="@+id/botaoContraIndicacoes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/botaoIndicacoes"
        android:layout_alignBottom="@+id/botaoIndicacoes"
        android:layout_toEndOf="@+id/textView22"
        android:layout_toRightOf="@+id/textView22"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="Contra-Indicações"
        android:textSize="12sp" />

    <Button
        android:id="@+id/id_botaoVoltar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="23dp"
        android:layout_toEndOf="@+id/botaoConvencional"
        android:layout_toRightOf="@+id/botaoConvencional"
        android:background="@drawable/fundo_caixa_texto"
        android:padding="5dp"
        android:text="Voltar para Inicio"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/botaoConvencional"
        android:layout_marginTop="14dp"
        app:srcCompat="@drawable/teste5" />

</RelativeLayout>