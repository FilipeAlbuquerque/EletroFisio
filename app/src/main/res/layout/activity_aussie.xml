<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="eletrofisio.com.teste11.activity.Aussie.AussieActivity">


    <TextView
        android:id="@+id/textView211"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp"
        android:text="Corrente Aussie"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView212"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView211"
        android:layout_marginTop="67dp"
        android:text="É uma nova geração de corrente elétrica para estimulação com vantagens sobre os tradicionais métodos de estimulação (Russa, Interferencial, TENS e FES)"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView213"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView212"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="44dp"
        android:text="Duas frequências portadoras: 1KHz e 4KHz"
        android:textSize="16sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/botaoIndic_Aussie"
        android:background="@drawable/fundo_caixa_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="indicações"
        android:layout_above="@+id/btContra_Aussie"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="17dp" />

    <Button
        android:id="@+id/btContra_Aussie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/fundo_caixa_texto"
        android:text="contra-indicações"
        android:layout_above="@+id/bt_Voltar_Aussie"
        android:layout_alignLeft="@+id/bt_Voltar_Aussie"
        android:layout_alignStart="@+id/bt_Voltar_Aussie"
        android:layout_marginBottom="41dp" />

    <Button
        android:id="@+id/bt_Voltar_Aussie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp"
        android:background="@drawable/fundo_caixa_texto"
        android:text="voltar para início" />

    <TextView
        android:id="@+id/textView214"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView213"
        android:layout_alignStart="@+id/textView213"
        android:layout_below="@+id/textView213"
        android:layout_marginTop="41dp"
        android:text="Modo de utilização e modulação semelhante a Corrente Russa"
        android:textSize="16sp"
        android:textStyle="bold" />

</RelativeLayout>
