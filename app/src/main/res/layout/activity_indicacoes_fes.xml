<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_indicacoes_fes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="eletrofisio.com.teste11.activity.Fes.IndicacoesFESActivity">

    <TextView
        android:text="INDICAÇÕES"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="55dp"
        android:id="@+id/textView42"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        tools:ignore="HardcodedText" />

    <TextView
        android:text="- Facilitação Neuromuscular;"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView42"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="51dp"
        android:id="@+id/textView47"
        android:textSize="20sp"
        android:textStyle="normal|bold" />

    <TextView
        android:text="- Controle da espasticidade;"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView49"
        android:textStyle="normal|bold"
        android:textSize="20sp" />

    <TextView
        android:text="- Paraplegias, parapesias (Hemiplegias, lesão medular etc);"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView49"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:id="@+id/textView50"
        android:textSize="20sp"
        android:textStyle="normal|bold" />

    <TextView
        android:text="- Substituição Órtica;"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView48"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:layout_above="@+id/textView49"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="28dp" />

    <TextView
        android:text="- Hipotrofia por desuso;"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView50"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp"
        android:id="@+id/textView51"
        android:textStyle="normal|bold"
        android:textSize="20sp" />

    <TextView
        android:text="- Esclerose Múltipla."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView51"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp"
        android:id="@+id/textView52"
        android:textStyle="normal|bold"
        android:textSize="20sp" />

    <Button
        android:text="voltar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/fundo_caixa_texto"
        android:layout_alignRight="@+id/textView51"
        android:layout_alignEnd="@+id/textView51"
        android:id="@+id/botaovoltar_fes" />
</RelativeLayout>
